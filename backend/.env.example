# This file serves as an example for the required environment variables.
# Copy this file to .env and fill in the values for your local development.

# The full connection string for your MongoDB database.
MONGODB_URI=mongodb+srv://<user>:<password>@<cluster-url>/<database-name>?retryWrites=true&w=majority

# The URL of the frontend application for CORS configuration.
FRONTEND_URL=http://localhost:5173

# The port the backend server will run on.
PORT=5000
